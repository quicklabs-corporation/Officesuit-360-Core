{
  "name": "offsuite",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && node --max_old_space_size=16000 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer",
    "start": "npm run env -s && node --max_old_space_size=16000 ./node_modules/@angular/cli/bin/ng serve",
    "dev-build": "npm run env -s && node --max_old_space_size=16000 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer --configuration=dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.8",
    "@angular/common": "~11.2.8",
    "@angular/compiler": "~11.2.8",
    "@angular/core": "~11.2.8",
    "@angular/forms": "~11.2.8",
    "@angular/platform-browser": "~11.2.8",
    "@angular/platform-browser-dynamic": "~11.2.8",
    "@angular/router": "~11.2.8",
    "@ant-design/icons": "^4.6.2",
    "ng-zorro-antd": "^11.3.0",
    "ng2-mqtt": "^0.1.2",
    "ngx-logger": "^4.2.1",
    "ngx-mqtt-client": "^1.3.4",
    "rxjs": "~6.6.0",
    "rxjs-compat": "^6.6.7",
    "tslib": "^2.0.0",
    "xlsx": "^0.17.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.7",
